{
  "name": "greaserocket",
  "description": "",
  "author": "Atte Huhtakangas <atte.huhtakangas@gmail.com>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint -c .eslintrc.js src",
    "lint:fix": "npm run lint -- --fix",
    "tslint": "tslint -c tslint.json src/**/*.tsx src/**/*.ts",
    "flow": "flow",
    "test": "NODE_ENV=test jest --forceExit",
    "test:precommit": "./pre-commit.sh",
    "coverage": "npm run test -- --coverage",
    "prettier": "prettier",
    "snapshot:update": "npm run test -- --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "lint-staged": "lint-staged",
    "download-schema": "apollo-codegen download-schema http://localhost:9000/graphql --output schema.json",
    "generate-graphql-types": "apollo-codegen generate '**/*.(graphql|js|tsx|ts|jsx)' --schema schema.json --target ts --output src/schema.ts",
    "generateSchema": "apollo-codegen introspect-schema http://localhost:9000/graphql --output schema.json"
  },
  "pre-commit": [
    "lint-staged",
    "test:precommit"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.71",
    "@types/node": "^8.0.19",
    "@types/react": "^16.0.0",
    "@types/react-native": "^0.47.0",
    "@types/react-navigation": "^1.0.16",
    "babel-plugin-module-resolver": "^2.7.1",
    "chroma-js": "^1.3.4",
    "color": "^2.0.0",
    "date-fns": "^1.28.5",
    "graphql": "^0.10.5",
    "idx": "^1.5.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "16.0.0-alpha.12",
    "react-apollo": "^1.4.10",
    "react-native": "0.47.1",
    "react-native-code-push": "^5.0.0-beta",
    "react-native-image-progress": "^1.0.1",
    "react-native-linear-gradient": "^2.1.0",
    "react-native-maps": "^0.15.3",
    "react-native-typescript-transformer": "^1.1.3",
    "react-native-vector-icons": "^4.3.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "redux-loop": "^2.2.2",
    "redux-persist": "^4.8.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "socket.io-client": "^2.0.3",
    "styled-components": "^2.1.2",
    "tsutils": "1.6.0",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/chroma-js": "^1.3.0",
    "@types/color": "^2.0.0",
    "@types/jest": "^20.0.6",
    "@types/react-native-vector-icons": "^4.2.1",
    "@types/react-redux": "^4.4.47",
    "@types/react-test-renderer": "^15.5.2",
    "@types/socket.io-client": "^1.4.29",
    "apollo-codegen": "^0.16.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-idx": "^1.5.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-react-native": "^2.1.0",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^3.0.1",
    "eslint-plugin-sort-class-members": "^1.2.0",
    "flow-bin": "^0.52.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-logger": "^3.0.6",
    "remote-redux-devtools": "^0.5.12",
    "ts-jest": "^20.0.9",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "^5.2.1",
    "tslint-config-prettier": "^1.3.0"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "coverage",
    "clearMocks": true,
    "setupFiles": [
      "./jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-navigation)"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --parser flow --write",
      "eslint --fix",
      "git add"
    ]
  }
}
